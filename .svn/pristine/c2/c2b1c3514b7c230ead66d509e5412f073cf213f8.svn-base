<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="5dp"
    tools:context=".AdverseEventsActivity"
    android:background="@color/color_fondo">

    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <TextView
            android:id="@+id/textViewTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_naranja"
            android:fontFamily="sans-serif-light"
            android:gravity="center"
            android:text="Evaluación eventos adversos"
            android:textColor="@color/color_white"
            android:textStyle="bold"
            android:textSize="26sp" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/body"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/header"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="20dp">

            <TextView
                android:id="@+id/txt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/color_transparent"
                android:fontFamily="sans-serif-light"
                android:text="Tratamiento"
                android:textStyle="bold"
                android:textColor="@color/color_azul_frio_2"
                android:textSize="24dp" />

            <Spinner
                android:id="@+id/spTratamiento"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt1"
                android:textColor="@color/color_azul_frio_2"
                android:textSize="24dp"
                />

            <RelativeLayout
                android:id="@+id/revlay1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/spTratamiento"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <TextView
                    android:id="@+id/txt2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta1"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Número de tratamientos recibidos (incluye el que se formuló):"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <EditText
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:ems="10"
                    android:id="@+id/numTratamientos"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/vineta1"
                    android:layout_alignRight="@id/txt2"
                    android:background="@color/color_white"
                    android:hint="numero"
                    android:textColorHint="@color/color_azul_frio_1"
                    android:textColor="@color/color_azul_frio_2"/>
            </RelativeLayout>

            <TextView
                android:id="@+id/txt3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay1"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:background="@color/color_transparent"
                android:fontFamily="sans-serif-light"
                android:text="Información del tratamiento recibido"
                android:textStyle="bold"
                android:textColor="@color/color_azul_frio_2"
                android:textSize="20dp" />

            <RelativeLayout
                android:id="@+id/revlay2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txt3"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_2">

                <ImageView
                    android:id="@+id/vineta2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <TextView
                    android:id="@+id/txt4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta2"
                    android:fontFamily="sans-serif-light"
                    android:text="Lugar donde fue administrado:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:id="@+id/lugarTratamiento"
                    android:layout_below="@id/txt4"
                    android:hint="lugar"
                    android:background="@color/color_white"
                    android:textColorHint="@color/color_azul_frio_1"
                    android:textColor="@color/color_azul_frio_2"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay2"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <EditText
                    android:id="@+id/fechaPrimeraDosis"
                    android:inputType="date"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/vineta3"
                    android:fontFamily="sans-serif-light"
                    android:hint="Fecha primera dosis:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textColorHint="@color/color_azul_frio_1"
                    android:textSize="20dp" />


                <ImageView
                    android:id="@+id/vineta4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_below="@id/fechaPrimeraDosis"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <EditText
                    android:id="@+id/fechaUltimaDosis"
                    android:inputType="date"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/fechaPrimeraDosis"
                    android:layout_toRightOf="@id/vineta4"
                    android:fontFamily="sans-serif-light"
                    android:hint="Fecha ultima dosis:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textColorHint="@color/color_azul_frio_1"
                    android:textSize="20dp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/revlay4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay3"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_2">

                <ImageView
                    android:id="@+id/vineta5"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <TextView
                    android:id="@+id/txt5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta5"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="¿El paciente recibió el tratamiento completo?:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spTratamientoCompleto"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/vineta5"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay4"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta6"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <TextView
                    android:id="@+id/txt6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta6"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="¿El paciente recibió el tratamiento durante el tiempo ordenado?:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spTratamientoTiempoOrdenado"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/vineta6"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay6"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay5"
                android:paddingTop="10dp"
                android:background="@color/color_white">

                <ImageView
                    android:id="@+id/vineta7"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta" />

                <TextView
                    android:id="@+id/txt7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta7"
                    android:fontFamily="sans-serif-light"
                    android:text="*Si respondió NO en alguna de las dos últimas preguntas, explique brevemente la razón."
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="14dp" />

                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="120dp"
                    android:scrollbars="vertical"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:id="@+id/razonPreguntas"
                    android:layout_below="@id/vineta7"
                    android:hint="razon"
                    android:background="@color/color_white"
                    android:textColorHint="@color/color_azul_frio_1"
                    android:textColor="@color/color_azul_frio_2"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/revlay7"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay6"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta8"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta8"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Dolor en el la inyección:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spDolorInyeccion"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt8"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay8"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay7"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_2">

                <ImageView
                    android:id="@+id/vineta9"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta9"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Infección en el sitio:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spInfeccionSitio"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt9"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay9"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay8"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta10"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta10"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Malestar General:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spMalGeneral"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt10"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay10"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay9"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_2">

                <ImageView
                    android:id="@+id/vineta11"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta11"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Mialgias:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spMialgias"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt11"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay11"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay10"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta12"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta12"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Artralgias:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spArtralgias"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt12"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay12"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay11"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_2">

                <ImageView
                    android:id="@+id/vineta13"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta13"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Cefalea:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spCefalea"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt13"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay13"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/revlay12"
                android:paddingBottom="0dp"
                android:paddingTop="10dp"
                android:background="@color/color_azul_calido_1">

                <ImageView
                    android:id="@+id/vineta14"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/icons_top_margin"
                    android:contentDescription="@string/create_patient"
                    android:src="@drawable/vineta"
                    android:visibility="invisible"/>

                <TextView
                    android:id="@+id/txt14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/vineta14"
                    android:background="@color/color_transparent"
                    android:fontFamily="sans-serif-light"
                    android:text="Fiebre:"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp" />

                <Spinner
                    android:id="@+id/spFiebre"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="0dp"
                    android:layout_alignRight="@+id/txt14"
                    android:layout_alignParentEnd="true"
                    android:fontFamily="sans-serif-light"
                    android:textColor="@color/color_azul_frio_2"
                    android:textSize="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/revlay14"
